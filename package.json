{
  "name": "tripsearch-client",
  "version": "1.0.0",
  "description": "A simple setup with webpack + node scripts",
  "main": "src/js/index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src *:watch",
    "open:src": "open http://tripsearch.dev",
    "sass:watch": "node-sass src/sass/app.scss public/css/app.css --watch",
    "webpack:watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaartenGDev/React-SCSS-starter.git"
  },
  "bugs": {
    "url": "https://github.com/MaartenGDev/React-SCSS-starter/issues"
  },
  "homepage": "https://github.com/MaartenGDev/React-SCSS-starter#readme",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "MaartenGDev",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "browser": "^0.2.6",
    "node-sass": "^3.10.0",
    "npm-run-all": "^3.1.0",
    "open": "0.0.5",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-google-maps": "^6.0.1",
    "react-helmet": "^3.1.0",
    "react-router": "^2.7.0"
  }
}
